- title current_object.title
- content_for :subnavigation do
  %li= link_to t(:'.index'), objects_path
  %li= link_to t(:'.edit'), edit_object_path(current_object)

%div[current_object]
  
  = link_to image_tag(current_object.photo_url(:medium)), current_object.photo_url, :rel => 'lightbox'
  
  .tags= @photo.tags.join(', ')
